{
  "name": "vue3-trimmer",
  "version": "0.0.1",
  "packageManager": "pnpm@8.3.0",
  "description": "vue3-trimmer",
  "author": "vue3-trimmer",
  "license": "MIT",
  "homepage": "https://github.com/vue3-trimmer/vue3-trimmer",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue3-trimmer/vue3-trimmer.git"
  },
  "bugs": "https://github.com/vue3-trimmer/vue3-trimmer/issues",
  "keywords": [
    "vue3-trimmer"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js"
    },
    "./style": "./dist/index.css"
  },
  "main": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "vite build --mode production",
    "build:watch": "vite build --watch",
    "dev": "vite",
    "play": "pnpm --filter playground dev",
    "unocss": "unocss dev --watch",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm build && bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@unocss/cli": "^0.56.4",
    "@unocss/transformer-compile-class": "^0.56.4",
    "@unocss/transformer-directives": "^0.56.4",
    "@unocss/transformer-variant-group": "^0.56.4",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/coverage-v8": "^0.34.3",
    "@vue-macros/volar": "^0.14.2",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/core": "^10.4.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "jsdom": "^22.1.0",
    "typescript": "^5.2.2",
    "unocss": "^0.56.4",
    "unplugin-vue-macros": "^2.4.14",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.34.3",
    "vue": "^3.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "vue3-grid-layout-next": "^1.0.6"
  }
}
